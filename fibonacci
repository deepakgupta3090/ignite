def fib(n):
    a= 1
    b=2
    total =b
    c = 0
    if n<2:
            return total
    for i in range(2,n):
       c=a + b
       if c <= 40000000:
           a=b
           b=c
           if c%2 ==0:
               total = total + c
       else:
           break
         
    return total
t = fib(35)
print t

